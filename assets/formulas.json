  {
    "title": "F1 Score",
    "category": "ML Metrics",
    "route": "/cards/ml-metrics/f1-score/",
    "formula_markdown": "\\[\nF_1 = \\frac{2\\,\\mathrm{Precision}\\,\\mathrm{Recall}}{\\mathrm{Precision}+\\mathrm{Recall}}\n\\]"
  },
  {
